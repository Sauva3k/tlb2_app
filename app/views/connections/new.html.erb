<% provide(:title, 'Sign up') %>
<h1>Activate Wifi</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
	  <%= form_for(:wifi, url: confirm_key_path) do |f| %>
	    <%= f.label :key, "Wifi Key" %>
        <%= f.text_field :key, class: 'form-control'%>
		<%= f.submit "Activate Wifi", class: "btn btn-primary" %>
	  <% end %>
  </div>
</div>